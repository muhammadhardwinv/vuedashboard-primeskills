    <div slot=\"header\">\r\n          <h4 class=\"card-title\">University in US</h4>\r\n        </div>\r\n        <div class=\"table-responsive table-full-width\">\r\n          <el-table class=\"table-striped\" :data=\"CRUDTables\">\r\n            <el-table-column label=\"name\" property=\"name\"></el-table-column>\r\n            <template slot-scope=\"props\">\r\n              <a\r\n                v-tooltip.top-center=\"'Like'\"\r\n                class=\"btn-info btn-simple btn-link\"\r\n                @click=\"handleLike(props.$index, props.row)\"\r\n              >\r\n                <i class=\"fa fa-heart\"></i\r\n              ></a>\r\n              <a\r\n                v-tooltip.top-center=\"'Edit'\"\r\n                class=\"btn-warning btn-simple btn-link\"\r\n                @click=\"handleEdit(props.$index, props.row)\"\r\n                ><i class=\"fa fa-edit\"></i\r\n              ></a>\r\n              <a\r\n                v-tooltip.top-center=\"'Delete'\"\r\n                class=\"btn-danger btn-simple btn-link\"\r\n                @click=\"handleDelete(props.$index, props.row)\"\r\n                ><i class=\"fa fa-times\"></i\r\n              ></a>\r\n            </template>\r\n            <el-table-column\r\n              label=\"domains\"\r\n              property=\"domains\"\r\n            ></el-table-column>\r\n            <el-table-column\r\n              label=\"Country\"\r\n              property=\"country\"\r\n            ></el-table-column>\r\n            <el-table-column\r\n              label=\"alpha_two_code\"\r\n              property=\"alpha_two_code\"\r\n            ></el-table-column>\r\n            <el-table-column\r\n              label=\"state-provinces\"\r\n              property=\"state-provinces\"\r\n            ></el-table-column>\r\n            <el-table-column\r\n              label=\"web_pages\"\r\n              property=\"web_pages\"\r\n            ></el-table-column>\r\n          </el-table>\r\n        </div>\r\n      </card>\r\n    </div>\r\n  </div>\r\n</template>\r\n<script>\r\nimport { Table, TableColumn } from \"element-ui\";\r\nimport axios from \"axios\";\r\nexport default {\r\n  components: {\r\n    [Table.name]: Table,\r\n    [TableColumn.name]: TableColumn,\r\n  },\r\n  data() {\r\n    return {\r\n      university: [],\r\n    };\r\n  },\r\n  created() {\r\n    this.getUniversity();\r\n  },\r\n  methods: {\r\n    getUniversity() {\r\n      const response = axios\r\n        .get(\"\", {})\r\n        .then((response) => {\r\n          console.log(response.data);\r\n          this.university = response.data;\r\n        })\r\n        .catch((error) => {\r\n          console.log(error);\r\n        });\r\n    },\r\n\r\n    tableRowClassName(row, index) {\r\n      if (index === 0) {\r\n        return \"success\";\r\n      } else if (index === 2) {\r\n        return \"info\";\r\n      } else if (index === 4) {\r\n        return \"danger\";\r\n      } else if (index === 6) {\r\n        return \"warning\";\r\n      }\r\n      return \"\";\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style></style>\r\n"]}]}