ible\"></slot>\n      <template v-else>\n        <slot name=\"dismiss-icon\">\n          <button\n            type=\"button\"\n            class=\"close\"\n            aria-label=\"Close\"\n            @click=\"dismissAlert\"\n          >\n            <i class=\"nc-icon nc-simple-remove\"></i>\n          </button>\n        </slot>\n\n        <template v-if=\"icon || $slots.icon\">\n          <slot name=\"icon\">\n            <span data-notify=\"icon\" :class=\"icon\"></span>\n          </slot>\n        </template>\n\n        <span data-notify=\"message\"> <slot></slot> </span>\n      </template>\n    </div>\n  </fade-transition>\n</template>\n<script>\nimport { FadeTransition } from 'vue2-transitions';\n\nexport default {\n  name: 'l-alert',\n  components: {\n    FadeTransition\n  },\n  props: {\n    type: {\n      type: String,\n      default: 'default',\n      description: 'Alert type'\n    },\n    dismissible: {\n      type: Boolean,\n      default: false,\n      description: 'Whether alert is dismissible (closeable)'\n    },\n    icon: {\n      type: String,\n      default: '',\n      description: 'Alert icon to display'\n    }\n  },\n  data() {\n    return {\n      visible: true\n    };\n  },\n  methods: {\n    dismissAlert() {\n      this.visible = false;\n    }\n  }\n};\n</script>\n"]}]}