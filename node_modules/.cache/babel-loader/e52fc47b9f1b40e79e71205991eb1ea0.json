ds: {\n    getData() {\n      const userData = JSON.parse(localStorage.getItem(\"data\"));\n    },\n    submit() {\n      alert(\"Form has been submitted!\");\n    },\n    onLogin() {\n      const response = axios\n        .post(\"https://api-lightspace.primeskills.id/api/auth/login\", {\n          email: this.email,\n          password: this.password,\n        })\n        .then((response) => {\n          window.alert(\"Login Success\");\n          // localStorage.setItem(response.data.token);\n          // console.log(response.data.data.token);\n          this.data = response.data.data.token;\n          localStorage.setItem(\"token\", response.data.data.token);\n        })\n        .catch((error) => {\n          console.log(error);\n          window.alert(\"Login Failed\");\n        });\n    },\n    toggleNavbar() {\n      document.body.classList.toggle(\"nav-open\");\n    },\n    closeMenu() {\n      document.body.classList.remove(\"nav-open\");\n      document.body.classList.remove(\"off-canvas-sidebar\");\n    },\n  },\n  beforeDestroy() {\n    this.closeMenu();\n  },\n};\n</script>\n<style>\n.navbar-nav .nav-item p {\n  line-height: inherit;\n  margin-left: 5px;\n}\n</style>\n"],"mappings":"AAsEA,SACAA,QAAA,IAAAC,SAAA,EACAC,oBAAA,QACA;AACA,OAAAC,UAAA;AACA,SAAAC,MAAA;AACA,SAAAC,QAAA,EAAAC,KAAA,EAAAC,GAAA;AACA,OAAAC,KAAA;AAEAJ,MAAA,UAAAE,KAAA;AACAF,MAAA,aAAAC,QAAA;AACAD,MAAA,QAAAG,GAAA;AAEA;EACAE,UAAA;IACAP,oBAAA,EAAAA,oBAAA;IACAC,UAAA,EAAAA;EACA;EACAO,IAAA,WAAAA,KAAA;IACA;MACAJ,KAAA;MACAK,QAAA;MACAC,SAAA;IACA;EACA;EAKAC,OAAA;IACAC,OAAA,WAAAA,QAAA;MACA,IAAAC,QAAA,GAAAC,IAAA,CAAAC,KAAA,CAAAC,YAAA,CAAAC,OAAA;IACA;IACAC,MAAA,WAAAA,OAAA;MACAC,KAAA;IACA;IACAC,OAAA,WAAAA,QAAA;MAAA,IAAAC,KAAA;MACA,IAAAC,QAAA,GAAAhB,KAAA,CACAiB,IAAA;QACAnB,KAAA,OAAAA,KAAA;QACAK,QAAA,OAAAA;MACA,GACAe,IAAA,WAAAF,QAAA;QACAG,MAAA,CAAAN,KAAA;QAGAE,KAAA,CAAAb,IAAA,GAAAc,QAAA,CAAAd,IAAA,CAAAA,IAAA,CAAAkB,KAAA;QACAV,YAAA,CAAAW,OAAA,UAAAL,QAAA,CAAAd,IAAA,CAAAA,IAAA,CAAAkB,KAAA;MACA,GACAE,KAAA,WAA